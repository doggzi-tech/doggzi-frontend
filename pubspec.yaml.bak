name: doggzi
description: "A new Flutter project."
publish_to: 'none'
version: 1.0.7+8

environment:
  sdk: ">=3.0.0 <4.0.0"

dependencies:
  flutter:
    sdk: flutter
  cupertino_icons: ^1.0.6
  get: ^4.6.6
  flutter_screenutil: ^5.9.0
  get_storage: ^2.1.1
  dio: ^5.4.3+1
  pretty_dio_logger: ^1.3.1
  firebase_core: ^2.32.0
  firebase_analytics: ^10.10.7
  firebase_crashlytics: ^3.5.7
  firebase_performance: ^0.9.4+7
  flutter_svg: ^2.0.10+1
  shimmer: ^3.0.0
  pinput: ^4.0.0
  fluttertoast: ^8.2.12
  zoom_tap_animation: ^1.1.0

dev_dependencies:
  flutter_test:
    sdk: flutter
  flutter_lints: ^3.0.0

flutter:
  uses-material-design: true

  assets:
    - assets/images/
  fonts:
    - family: Manrope
      fonts:
        - asset: assets/fonts/manrope-regular.ttf
        - asset: assets/fonts/manrope-medium.ttf
          weight: 500
        - asset: assets/fonts/manrope-semiBold.ttf
          weight: 600